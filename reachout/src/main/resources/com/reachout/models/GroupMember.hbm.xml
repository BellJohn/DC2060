<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.reachout.models">

	<class name="GroupMember" table="GROUP_MEMBER">
		<id name="id" column="GM_ID">
			<generator class="increment" />
		</id>
		<property name="groupId" type="int" column="GM_G_ID" />
		<property name="userId" type="int" 	column="GM_G_UID" />
		<property name="userStatus" type="int" column="GM_U_STATUS_ID" />
	</class>

</hibernate-mapping>
