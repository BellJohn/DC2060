<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping
	package="com.reachout.models">

	<class name="UserProfile" table="USERPROFILE">
		<id name="userId" column="USERS_ID">
		</id>
		<property name="bio" type="string" column="BIO" />
		<property name="profilePic" type="string" column="PROFILE_PIC" />
		<property name="healthStatus" type="string" column="HEALTH_STATUS" />
	</class>

</hibernate-mapping>
